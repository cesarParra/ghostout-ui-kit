{
  "name": "ghostout-ui-kit",
  "version": "1.11.0",
  "description": "ghostout-ui-kit React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-js": "nwb build-react-component",
    "build": "npm-run-all build-css build-js",
    "clean": "nwb clean-module && nwb clean-demo",
    "start-js": "nwb serve-react-demo",
    "start": "npm-run-all -p watch-css start-js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "chai": "^4.1.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-test-renderer": "^16.5.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "nwb": "0.21.x",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "",
  "homepage": "https://github.com/cesarParra/ghostout-ui-kit",
  "license": "MIT",
  "repository": "https://github.com/cesarParra/ghostout-ui-kit",
  "keywords": [
    "react-component"
  ]
}
