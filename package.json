{
  "name": "ghostout-ui-kit",
  "version": "0.0.3",
  "description": "Describe ghostout-ui-kit here",
  "private": false,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "nwb serve-react-app",
    "build-js": "nwb build-react-app",
    "build": "npm-run-all build-css build-js",
    "clean": "nwb clean-app",
    "start": "npm-run-all -p watch-css start-js",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "nwb": "0.21.x"
  },
  "author": "cesarParra",
  "license": "MIT",
  "repository": ""
}
